<h1>List of Members</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Membership Number</th>
    <th>Discount</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.membership %></td>
    <td><%= user.discount %></td>
    <td><%= link_to 'Show' %></td>
    <td><%= link_to 'Edit' %></td>
    <td><%= link_to 'Destroy', '#', method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

